    "import turtle\n",
    "import math\n",
    "alex = turtle.Turtle()\n",
    "lista = [(0, 100), (135, 100 * math.sqrt(2)), (-135, 100), (-135, 100 * math.sqrt(2)), (-135, 100), (-30, 100), (-120, 100), (-30, 100)]\n",
    "for (angulo, dist) in lista:\n",
    "    alex.left(angulo)\n",
    "    alex.forward(dist)"
